class Solution {
    public int mostWordsFound(String[] sentences) {
        int count =0;
        int max = 0;
        for(int i=0;i<sentences.length;i++){
            String a = sentences[i];
            String res[] = a.split(" ");
            count = res.length;
            if(count>max){
                max = count;
            }
        }
        return max;
    }
}
